{
  "bare": true,
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": [
        "save"
      ]
    },
    "@vue/cli-plugin-unit-jest": {},
    "@vue/cli-plugin-e2e-cypress": {},
    "vue-cli-plugin-vuetify": {
      "preset": "configure",
      "replaceComponents": false,
      "useTheme": false,
      "useCustomProperties": false,
      "iconFont": "mdi",
      "installFonts": false,
      "useAlaCarte": true,
      "usePolyfill": true,
      "locale": "en"
    },
    "vue-cli-plugin-eslint-config-vuetify": {},
    "vue-cli-plugin-vuetify-essentials": {},
    "vue-cli-plugin-meta": {
      "projectName": "Vuetify",
      "url": "",
      "description": "An Opinionated Pre-configured Vuetify application",
      "twitterHandle": "vuetifyjs",
      "socialImage": "https://cdn.vuetifyjs.com/images/logos/vuetify-logo-300.png",
      "googleAnalytics": ""
    },
    "vue-cli-plugin-i18n": {
      "locale": "en",
      "fallbackLocale": "en",
      "localeDir": "locales",
      "enableInSFC": false
    }
  },
  "router": true,
  "routerHistoryMode": true,
  "vuex": true,
  "cssPreprocessor": "dart-sass",
  "configs": {
    "vue": {
      "devServer": {
        "disableHostCheck": true
      },
      "transpileDependencies": ["vuetify"],
      "css": {
        "loaderOptions": {
          "sass": {
            "data": "@import \"~@/sass/main.scss\""
          }
        }
      }
    }
  }
}
