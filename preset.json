{
  "bare": true,
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": [
        "save"
      ]
    },
    "@vue/cli-plugin-unit-jest": {},
    "@vue/cli-plugin-e2e-cypress": {},
    "vue-cli-plugin-vuetify": {
      "preset": "configure",
      "replaceComponents": false,
      "useTheme": true,
      "useCustomProperties": false,
      "iconFont": "mdi",
      "installFonts": false,
      "useAlaCarte": true,
      "usePolyfill": true,
      "locale": "en"
    },
    "vue-cli-plugin-eslint-config-vuetify": {},
    "vue-cli-plugin-vuetify-essentials": {}
  },
  "router": true,
  "routerHistoryMode": true,
  "vuex": true,
  "cssPreprocessor": "sass",
  "configs": {
    "vue": {
      "devServer": {
        "disableHostCheck": true
      }
    }
  }
}
